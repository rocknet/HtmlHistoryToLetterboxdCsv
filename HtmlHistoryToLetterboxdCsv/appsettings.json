{
  "HtmlHistoryToLetterboxdConfig": {
    "ExportFolder": "Export/",
    "Processors": {
      "Amazon": {
        "Tags": "AmazonPrime",
        "WatchedDateAddDays": 0,
        "WatchedDateFormat": "MMMM d, yyyy",
        "XPaths": {
          "ItemList": "//div[@data-automation-id='activity-history']/div[@data-automation-id='activity-history-items']/ul",
          "Items": "li/ul/li/div/div/div/a",
          "DateSelectorAncestorName": "ul",
          "DateSelector": "../div/div"
        }
      },
      "NetflixDvd": {
        "Tags": "NetflixDVD",
        "WatchedDateAddDays": -3,
        "WatchedDateFormat": "MM/dd/yy",
        "XPaths": {
          "ItemList": "//div[@id='historyList']/ul",
          "Items": "li[not(@class='heading')]/div[@class='title'][not(p/span[contains(., 'Season')])]/p/a",
          "DateSelectorAncestorName": "li",
          "DateSelector": "div[@class='returned']/text()"
        }
      },
      "Plex": {
        "Tags": "Plex",
        "WatchedDateAddDays": 0,
        "WatchedDateFormat": "h:mmtt MMM d, yyyy",
        "XPaths": {
          "ItemList": "//div[@class[contains(., 'PlayHistoryList')]]/div",
          "Items": "div[descendant::div[span[.='Movie']]]//span[@class[contains(., 'PlayHistoryTableTitle')]]",
          "DateSelectorAncestorName": "",
          "DateSelector": "ancestor::*[4]/div[last()]/span"
        }
      }
    }
  }
}